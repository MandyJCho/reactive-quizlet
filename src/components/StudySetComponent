};

const defaultProps = {
  name: 'Study Set Name',
  cards: [{term: 'term', definition: 'definition'}],
};

export default function StudySetComponent() {
  return (
    <DeckContainer>

    </DeckContainer>
  );
}

StudySetComponent.propTypes = propTypes;
StudySetComponent.defaultProps = defaultProps;
